{
  "name": "nextjs-starter",
  "description": "A Next.js starter that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "fuad fuad<fuadnafiz98@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "dev": "next",
    "start": "next start",
    "export": "next export",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "generate:prisma": "prisma generate --schema=./src/server/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "gen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/server/graphql/schema.ts",
    "type-check": "tsc",
    "studio": "prisma studio --schema=./src/server/prisma/schema.prisma",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@prisma/client": "3.2.1",
    "apollo-server-micro": "3.3.0",
    "graphql": "15.5.3",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "nexus-plugin-prisma": "0.35.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "urql": "2.0.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-urql": "3.2.0",
    "@graphql-codegen/urql-introspection": "2.1.0",
    "@tailwindcss/forms": "0.3.3",
    "@types/micro": "7.3.6",
    "@types/micro-cors": "0.1.2",
    "@types/node": "16.9.2",
    "@types/prettier": "2.3.2",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "10.3.4",
    "eslint": "7.32.0",
    "eslint-config-alloy": "4.3.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-react": "7.25.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "12.0.2",
    "lint-staged": "11.1.2",
    "next-sitemap": "1.6.168",
    "postcss": "8.3.6",
    "prettier": "2.4.1",
    "prisma": "3.2.1",
    "tailwindcss": "2.2.15",
    "ts-node": "10.2.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.3"
  }
}
