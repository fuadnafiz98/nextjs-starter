{
  "name": "nextjs-starter",
  "description": "A Next.js starter that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "fuad fuad<fuadnafiz98@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "generate:prisma": "prisma generate --schema=./src/server/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\""
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@prisma/client": "2.27.0",
    "apollo-server-micro": "3.0.2",
    "graphql": "15.5.1",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "latest",
    "nexus": "1.1.0",
    "nexus-plugin-prisma": "0.35.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.3",
    "@types/micro": "7.3.6",
    "@types/micro-cors": "0.1.2",
    "@types/node": "15.12.2",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "autoprefixer": "10.2.6",
    "eslint": "7.28.0",
    "eslint-config-alloy": "4.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "lint-staged": "11.0.0",
    "postcss": "8.3.0",
    "prettier": "2.3.1",
    "prisma": "2.27.0",
    "tailwindcss": "2.1.4",
    "ts-node": "10.1.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.2"
  }
}
